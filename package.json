{
  "name": "nuxt3-nestjs-boilerplate",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/piscis/nuxt3-nestjs-boilerplate.git",
    "type": "git"
  },
  "engines": {
    "pnpm": "7.x",
    "node": "18.x"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm dlx husky install",
    "test": "nuxi test",
    "test:watch": "nuxi test --dev --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nuxt/test-utils": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@vueuse/nuxt": "^9.5.0",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.0.0",
    "nuxt-icon": "^0.1.7",
    "nuxt-security": "^0.10.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "lint-staged": {
    "*.{vue,ts}": [
      "pnpm run lint"
    ]
  }
}

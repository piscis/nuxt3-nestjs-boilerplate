{
  "name": "nuxt3-nestjs-boilerplate",
  "private": true,
  "packageManager": "pnpm@8.6.2",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/piscis/nuxt3-nestjs-boilerplate.git",
    "type": "git"
  },
  "engines": {
    "pnpm": "8",
    "node": "18"
  },
  "scripts": {

    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/testing": "^9.4.2",
    "@nuxt/test-utils": "^3.5.3",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@vueuse/nuxt": "^10.1.2",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.3",
    "nuxt-icon": "^0.4.1",
